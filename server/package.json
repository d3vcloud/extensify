{
  "name": "extensify-api",
  "version": "1.0.0",
  "description": "Server for extensify extension",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsnd --respawn src/index.ts",
    "start": "node src/dist/index.js",
    "prettier:check": "npx prettier . --check",
    "prettier:fix": "npx prettier . --write",
    "model:generate": "npx prisma generate",
    "db:migrate": "npx prisma db push",
    "seed": "tsnd --respawn src/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "4.1.1",
    "cors": "2.8.5",
    "dotenv": "16.0.1",
    "express": "4.18.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/node": "18.6.3",
    "prisma": "4.1.1",
    "ts-node-dev": "2.0.0"
  }
}
